<%= link_to "Jokes Home", jokes_path %>
<%= link_to "New Joke", new_joke_path %>


<h1>Tell me more bout that funny</h1>
<%="#{@joke.id}" %>.
<%="#{@joke.title}"%>
<%="#{@joke.updated_at}" %>

  <%=form_for @vote, :url => votes_path(:id => @joke.id) do |f| %>
      Vote Count - <%=@joke.votes.length%> -
  <%= f.submit %>
<%end%>
