<%= link_to "Jokes Home", jokes_path %>
<%= link_to "New Joke", new_joke_path %>


<h1>Tell me more bout that funny</h1>
<%="#{@joke.id}" %>.
<%="#{@joke.title}"%>
<%="#{@joke.updated_at}" %>

  <%=form_for @vote, :url => votes_path(:id => @joke.id, :up => true) do |f| %>
  <%= f.submit "Up" %>
  <%end%>
  <%= @joke.votes.length %>
  <%=form_for @vote, :url => votes_path(:id => @joke.id, :up => false) do |f| %>
  <%= f.submit "Down" %>
<%end%>
