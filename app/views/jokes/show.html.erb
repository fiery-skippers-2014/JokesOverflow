<header>
  <%= link_to "Jokes Home", jokes_path %>
  <%= link_to "New Joke", new_joke_path %>
</header>


<h1>Tell me more bout that funny</h1>
<%="#{@joke.id}" %>.
<%="#{@joke.title}"%>
<%="#{@joke.updated_at}" %>


<%=form_for @vote, :url => votes_path(:id => @joke.id, :up => true), :remote => true do |f| %>
  <%= f.submit "Up" %>
<% end %>

<div class="votes">
  <%= @joke.votes.length %>
</div>

<%=form_for @vote, :url => votes_path(:id => @joke.id, :up => false), :remote => true do |f| %>
  <%= f.submit "Down" %>
<%end%>

<%= link_to "Reply", new_joke_reply_path(@joke), class: 'button', :remote => true %>

<div class= "replyform"></div>

<%= render partial: 'partials/all_replies', locals: { all_replies: @all_replies} %>




